<!DOCTYPE html>
<html lang="en">
  <% include ./partials/head.ejs %>
  <body>
        <% if(wikis) { %>
            <section class='row'>
                <section class='col-sm-12'>
                    <div class="featured-wiki"style='background: url("data:image/jpeg;base64, <%= Buffer.from(wikis[wikis.length - 1].images[0].data).toString('base64') %> ")'></div>
                </section>
            </section>
            <section class='row'>
                <section class='col-sm-12 align-self-left'>
                    <h4><%= wikis[wikis.length - 1].title %></h4>
                </section>
            </section>
            <section class='row'>
                <section class='col-sm-12 align-self-left'>
                    <p class='text-truncate'><%= wikis[wikis.length - 1].body %></p>
                </section>
            </section>
        <% } %>       
    <% include ./partials/baseScripts.ejs %>
  </body>
</html>